{
  "firestore": {
    "database": "writebook",
    "location": "nam5",
    "rules": "firestore/firestore.rules",
    "indexes": "firestore/firestore.indexes.json"
  },
  "storage": {
    "rules": "storage/storage.rules"
  },
  "hosting": {
    "site": "comfyshare",
    "public": "comfyshare-app/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/404.html"
      }
    ]
  },
  "projectEnvironments": {
    "starmind-72daa": {
      "NEXT_PUBLIC_FIREBASE_API_KEY": "REPLACE_WITH_API_KEY",
      "NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN": "REPLACE_WITH_AUTH_DOMAIN",
      "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "starmind-72daa",
      "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": "REPLACE_WITH_STORAGE_BUCKET",
      "NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "REPLACE_WITH_SENDER_ID",
      "NEXT_PUBLIC_FIREBASE_APP_ID": "REPLACE_WITH_APP_ID"
    }
  }
}
